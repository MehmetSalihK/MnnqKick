{
  "action": {
    "default_icon": "images/icon_off_128.png",
    "default_popup": "index.html",
    "default_title": "mnnq - ÇEVRİMDIŞI"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "js": [
        "js/jquery.js",
        "js/config.js",
        "js/functions.js",
        "js/app.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "KICK.COM/MNNQ kanalı canlı yayına geçtiğinde anında bildirim alın!",
  "host_permissions": [
    "https://*.kick.com/*",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "images/icon_off_128.png",
    "64": "images/icon_off_64.png"
  },
  "manifest_version": 3,
  "name": "mnnq Bildirimi !",
  "permissions": [
    "storage",
    "activeTab",
    "notifications"
  ],
  "version": "0.1"
}